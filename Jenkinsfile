pipeline{
    agent any
    stages {
        stage('Init'){
            steps{
                echo 'Hi this is Yash'
                echo 'We are about to build the project'
            }
        }
        stage('Build'){
            steps{
                echo 'Building the project'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying to staging enviornment'
            }
        }
    }
        
}